print 'plugins'

lazypath = vim.fn.stdpath'data' .. '/lazy/lazy.nvim'
unless vim.loop.fs_stat lazypath
  vim.fn.system
    * 'git'
    * 'clone'
    * '--filter=blob:none'
    * 'https://github.com/folke/lazy.nvim.git'
    * '--branch=stable'
    * lazypath
vim.opt.rtp\prepend lazypath

opts =
  install:
    colorscheme:
      * 'habamax'

